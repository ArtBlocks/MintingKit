require('dotenv')
Dotenv.load '../.env'

app_identifier(ENV["FASTLANE_APP_IDENTIFIER"]) # The bundle identifier of your app
apple_id(ENV["FASTLANE_APPLE_ID"]) # Your Apple Developer Portal username

itc_team_id(ENV["FASTLANE_TEAM_ID"]) # App Store Connect Team ID
team_id(ENV["FASTLANE_DEV_PORTAL_ID"]) # Developer Portal Team ID

